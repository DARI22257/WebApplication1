<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:AvaloniaApplication2.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaApplication2.Views.EmployeesView"
        Title="EmployeesView"
        x:DataType="viewModels:EmployeesViewModel">
    <TabItem >
        <DockPanel>
            
            <Grid>
                <Button Content="Добавить" Background="Black" Foreground="White" Click="Add"/>
                <Button Content="Редактировать" Background="Black" Foreground="White" Click="Create"/> 
                <Button Content="Удалить" Background="Black" Foreground="White" Click="Delete"/>
                
            </Grid>
        </DockPanel>
    </TabItem>
        
</Window>
